		local message = table.concat({...}, " ")
		print(message)
	
		local txtlogBase = contents.ScrollingFrame.TextBox
		txtlogBase.Text = txtlogBase.Text .. "\n" .. message
	end
	
	local function getIfCusCilentEnv()
		local exec_name = (getexecutor or getexecutorname or getidentityexecutor or identifyexecutor or function() return 'Unknown' end)()
		return exec_name
	end
	
