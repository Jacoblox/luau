local function OJLNYU_fake_script() -- base.LocalScript 
	local script = Instance.new('LocalScript', base)

	local UserInputService = game:GetService("UserInputService")
	
	local gui = script.Parent
	
	local dragging
	local dragInput
	local dragStart
	local startPos
	
	local function update(input)
		local delta = input.Position - dragStart
		gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
	end
	
	gui.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = gui.Position
			
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	gui.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)
	
	UserInputService.InputChanged:Connect(function(input)
		if input == dragInput and dragging then
			update(input)
		end
	end)
end
coroutine.wrap(OJLNYU_fake_script)()
local function TLEIUDQ_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	local on = false
	
	script.Parent.MouseButton1Click:Connect(function()
		if on == false then
			on = true
			script.Parent.Parent.maincontents.Visible = false
			script.Parent.BackgroundColor3 =Color3.new(0,1,0)
			script.Parent.Text = 'o'
		else
			on = false
			script.Parent.Parent.maincontents.Visible = true
			script.Parent.BackgroundColor3 = Color3.new(1,0,0)
			script.Parent.Text = 'x'
		end
	end)
end
coroutine.wrap(TLEIUDQ_fake_script)()
local function REBAIE_fake_script() -- ctil.main 
	local script = Instance.new('LocalScript', ctil)

	-- CUtil main script!
	local base = script.Parent.base
	local gui = script.Parent
	local contents = base.maincontents
	
