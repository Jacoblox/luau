		msg = '[IDK] ' .. getIfCusCilentEnv()
	end
	
	log(msg)
	
	
	contents.logexecname.MouseButton1Click:Connect(function()
		log('Executor Name: ' .. getIfCusCilentEnv())
	end)
	
	
	
	contents.outlineallhrp.MouseButton1Click:Connect(function()
		if outlinedallhrp == false then
